    "elements": {
      "title": {
        "show": true,
        "content": "CONCAT",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "12px",
          "fill": "black"
        }
      },
      "symbol": {
        "show": true,
        "content": "â§º",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "shape.height"
        },
        "styles": {
          "font-size": "40px",
          "fill": "darkorange"
        }
      },
      "id": {
        "show": true,
        "content": "shape.id",
        "position": {
          "left": 0,
          "top": "shape.height-socket.height",
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
