  "components": [
    "string_gate"
  ],
  "behaviours": [
    "ConcatGate"
  ],
  "properties": [
    {
      "name": "lhs",
      "data_type": "string",
      "socket_type": "input"
    },
    {
      "name": "rhs",
      "data_type": "string",
      "socket_type": "input"
    },
    {
      "name": "result",
      "data_type": "string",
      "socket_type": "output"
    }
  ],
  "shape": {
    "width": 350,
