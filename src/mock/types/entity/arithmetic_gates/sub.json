{
  "name": "sub",
  "group": "arithmetic",
  "description": "Subtracts two integer numbers",
  "components": [
    "arithmetic_gate",
    "flow_2d",
    "flow_3d"
  ],
  "behaviours": [
    "SubGate"
  ],
  "palette": {
    "className": "inexor-ecs-minus-1"
  },
  "shape": {
    "group": "arithmetic",
    "width": 200,
    "socket": {
      "width": 60,
      "height": 30,
      "offset": 5
    },
    "offset": {
      "top": "socket.height",
      "bottom": "socket.height"
    },
    "elements": {
      "title": {
        "show": true,
        "content": "element.description",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "12px",
          "color": "black"
        }
      },
      "symbol": {
        "show": true,
        "content": "-",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "shape.height"
        },
        "styles": {
          "font-size": "40px",
          "fill": "steelblue"
        }
      },
      "id": {
        "show": true,
        "content": "shape.id",
        "position": {
          "left": 0,
          "top": "shape.height-socket.height",
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "9px",
          "color": "black"
        }
      }
    }
  }
}
