    },
    {
      "name": "input",
      "data_type": "string",
      "socket_type": "input"
    },
    {
      "name": "output",
      "data_type": "string",
      "socket_type": "output"
    }
  ],
  "palette": {
    "imgUrl": "/symbols/interfaces/websocket.svg",
    "className": "bpmn-icon-task-none"
