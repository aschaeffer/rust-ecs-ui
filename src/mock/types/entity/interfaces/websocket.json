          "color": "black"
        }
      },
      "symbol": {
        "show": true,
        "type": "svg",
        "symbol": "./interfaces/websocket.svg",
        "position": {
          "left": 750,
          "top": 300,
          "width": 600,
          "height": 600,
          "scale": 0.1
        },
        "styles": {
          "fill": "darkorange",
          "stroke": "rgba(0,0,0,0.2)",
          "strokeWidth": 2
        }
      },
      "id": {
        "show": true,
        "type": "text",
        "content": "shape.id",
        "position": {
          "left": 0,
          "top": "shape.height-socket.height",
