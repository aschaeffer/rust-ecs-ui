{
  "name": "vec3_to_numbers",
  "group": "converter",
  "description": "Vec3 to Numbers",
  "components": [
    "flow_2d",
    "flow_3d"
  ],
  "behaviours": [
    "AddGate"
  ],
  "properties": [
    {
      "name": "vec3",
      "data_type": "array",
      "socket_type": "input"
    },
    {
      "name": "x",
      "data_type": "number",
      "socket_type": "output"
    },
    {
      "name": "y",
      "data_type": "number",
      "socket_type": "output"
    },
    {
      "name": "z",
      "data_type": "number",
      "socket_type": "output"
    }
  ],
  "palette": {
    "content": "⎡x⎤&nbsp;&nbsp;\n⎢y⎥=>\n⎣z⎦&nbsp;&nbsp;",
    "styles": {
      "font-family": "Fira Code",
      "font-size": "12px",
      "line-height": "16px",
      "padding": "5px"
    }
  },
  "shape": {
    "width": 200,
    "socket": {
      "width": 60,
      "height": 30,
      "offset": 5
    },
    "offset": {
      "top": "socket.height",
      "bottom": "socket.height"
    },
    "elements": {
      "title": {
        "show": true,
        "type": "text",
        "content": "element.description",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "12px",
          "color": "black"
        }
      },
      "symbol": {
        "show": true,
        "type": "text",
        "content": "⎡ x ⎤\n⎢ y ⎥\n⎣ z ⎦",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "shape.height"
        },
        "styles": {
          "fill": "steelblue",
          "font-family": "Fira Code",
          "font-size": "20px"
        }
      },
      "id": {
        "show": true,
        "type": "text",
        "content": "shape.id",
        "position": {
          "left": 0,
          "top": "shape.height-socket.height",
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "9px",
          "color": "black"
        }
      }
    }
  }
}
