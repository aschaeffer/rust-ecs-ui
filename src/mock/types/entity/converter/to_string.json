{
  "name": "to_string",
  "group": "converters",
  "description": "Converts any input to a string",
  "components": [
    "converter",
    "flow_2d",
    "flow_3d"
  ],
  "properties": [
    {
      "name": "input",
      "data_type": "any",
      "socket_type": "input"
    },
    {
      "name": "result",
      "data_type": "string",
      "socket_type": "output"
    }
  ],
  "palette": {
    "content": "=>S",
    "styles": {
      "font-family": "Fira Code",
      "font-size": "16px",
      "padding": "5px"
    }
  },
  "shape": {
    "width": 200,
    "socket": {
      "width": 60,
      "height": 30,
      "offset": 5
    },
    "offset": {
      "top": "socket.height",
      "bottom": "socket.height"
    },
    "elements": {
      "title": {
        "show": true,
        "content": "element.description",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "12px",
          "color": "black"
        }
      },
      "symbol": {
        "show": true,
        "content": "=>S",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "shape.height"
        },
        "styles": {
          "fill": "darkorange",
          "font-family": "Fira Code",
          "font-size": "40px"
        }
      },
      "id": {
        "show": true,
        "content": "shape.id",
        "position": {
          "left": 0,
          "top": "shape.height-socket.height",
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "9px",
          "color": "black"
        }
      }
    }
  }
}
