{
  "name": "to_string",
  "description": "Converts any input to a string",
  "components": [
    "converter"
  ],
  "properties": [
    {
      "name": "input",
      "data_type": "any",
      "socket_type": "input"
    },
    {
      "name": "result",
      "data_type": "string",
      "socket_type": "output"
    }
  ],
  "shape": {
    "width": 200,
    "socket": {
      "width": 60,
      "height": 30
    },
    "offset": {
      "top": "socket.height",
      "bottom": "socket.height"
    },
    "elements": {
      "title": {
        "show": true,
        "content": "TO_STRING",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "12px",
          "color": "black"
        }
      },
      "symbol": {
        "show": true,
        "content": "str()",
        "position": {
          "left": 0,
          "top": 0,
          "width": "shape.width",
          "height": "shape.height"
        },
        "styles": {
          "font-size": "40px",
          "fill": "darkorange"
        }
      },
      "id": {
        "show": true,
        "content": "shape.id",
        "position": {
          "left": 0,
          "top": "shape.height-socket.height",
          "width": "shape.width",
          "height": "socket.height"
        },
        "styles": {
          "font-size": "9px",
          "color": "black"
        }
      }
    }
  }
}
